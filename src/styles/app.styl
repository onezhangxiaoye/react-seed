@import './dataList.styl';
@import './catalog.styl';
@import './header.styl';
@import './imgDataList.styl';
@import './AddImgData.styl';
@import './test3.styl';
.App
  width 100%
  height 100vh
  .App-body
    height 90%
    position: relative
    >div
      height 100%
      float left
      .Body-header
        height 100%
        background-color #cccccc
        display flex
        flex-direction column
        align-items center
        justify-content center
        font-size calc(10px + 2vmin)
        color white
        position relative
        overflow hidden
        .move-img
          position absolute
          top 0px
          transition left 0.5s, top 0.5s, transform 0.5s
          width 150px
        .Body-logo
            animation: Body-logo-spin infinite 20s linear
            height: 40vmin
        .animation-btn
          padding 0 20px
          background-color #FFF
          border-radius 6px
          color black
          line-height 40px
          text-align center
          margin-top 10px
          cursor pointer
          position absolute
          z-index 1
          bottom 40px
@keyframes Body-logo-spin {
  from {
    transform: rotate(0deg);
  }
  to {
    transform: rotate(360deg);
  }
}